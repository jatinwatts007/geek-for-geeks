#include<iostream>
using namespace std;
int main()
 {
     int t;
     cin>>t;
     while(t--){
         int n,i;
         cin>>n;
         int *arr = new int [n];
         for(i=0;i<n;i++){
             cin>>arr[i];
         }
         for(i=0;i<n;i++){
             if(arr[i]!=arr[i+1]){
                 cout<<arr[i];
                 break;
             }
             i++;
         }
         cout<<endl;
     }
	//code
	return 0;
}
